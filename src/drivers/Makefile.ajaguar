# Linear format detection
#
#ifeq ($(MWPIXEL_FORMAT), $(filter $(MWPIXEL_FORMAT), MWPF_TRUECOLORABGR MWPF_TRUECOLORARGB))
##ifeq ($(MWPIXEL_FORMAT), MWPF_TRUECOLORABGR)
ObjDRIVERS += $(O_DRIVERS_DIR)/fblin32.o
ObjDRIVERS_d += $(O_DRIVERS_DIR)/fblin32.d
##else ifeq ($(MWPIXEL_FORMAT), MWPF_TRUECOLORARGB)
##ObjDRIVERS += $(O_DRIVERS_DIR)/fblin32.o
##ObjDRIVERS_d += $(O_DRIVERS_DIR)/fblin32.d
#endif
#ifeq ($(MWPIXEL_FORMAT), MWPF_TRUECOLORRGB)
