ObjDRIVERS_d += $(O_DRIVERS_DIR)/fblin2.d
#endif
#ifeq ($(SCREEN_DEPTH), 1)
ObjDRIVERS += $(O_DRIVERS_DIR)/fblin1.o
ObjDRIVERS_d += $(O_DRIVERS_DIR)/fblin1.d
#endif
#
# Portrait detection
#
ifeq ($(HAVE_PORTRAIT_SUPPORT), Y)
ObjDRIVERS		+=	$(O_DRIVERS_DIR)/fbportrait_right.o $(O_DRIVERS_DIR)/fbportrait_left.o $(O_DRIVERS_DIR)/fbportrait_down.o
