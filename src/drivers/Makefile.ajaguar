ObjDRIVERS_d += $(O_DRIVERS_DIR)/fblin16.d
##else ifeq ($(MWPIXEL_FORMAT), MWPF_TRUECOLOR565)
##ObjDRIVERS += $(O_DRIVERS_DIR)/fblin16.o
##ObjDRIVERS_d += $(O_DRIVERS_DIR)/fblin16.d
#endif
