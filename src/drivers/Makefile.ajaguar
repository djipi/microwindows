ObjDRIVERS_d += $(O_DRIVERS_DIR)/fblin24.d
#endif
#ifeq ($(MWPIXEL_FORMAT), $(filter $(MWPIXEL_FORMAT), MWPF_TRUECOLOR555 MWPF_TRUECOLOR565))
