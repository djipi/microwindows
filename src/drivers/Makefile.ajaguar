endif


# drivers
#
compile_drivers:	$(ObjDRIVERS)
#
# Include the dependence file
include $(ObjDRIVERS_d)
#
$(O_DRIVERS_DIR)/vtswitch.d:
$(O_DRIVERS_DIR)/vtswitch.o: $(C_DRIVERS_DIR)/vtswitch.c $(O_DRIVERS_DIR)/vtswitch.d $(MW_BASE_SRC)/config $(MW_AJAGUAR_PRJ)/makefile $(C_DRIVERS_DIR)/Makefile.ajaguar
	$(CCProg) $< $(CCOpt) $(CCDefine) $(SDK_INC) $(CC_INC) $(H_INC) $(WIN_INC) -o $@
ifeq ($(GET_SFromC),yes)	
	$(CCProg) $< $(CCOpt) $(GET_SFromC_Option) $(CCDefine) $(SDK_INC) $(CC_INC) $(H_INC) $(WIN_INC) -o $(S_DRIVERS_DIR)/vtswitch.s
endif
